@keyframes blick
    0%
        left -100%
    100%
        left 200%

.title
    font-size 4.5em
    color rgba(255, 255, 255, 0)
    text-transform uppercase
    transition all 1s linear
    position relative
    -moz-user-select none
    -khtml-user-select none
    -webkit-user-select none
    -ms-user-select none
    user-select none
    margin 0
    padding 0
    font-family 'Exo2-300', sans-serif
    font-weight 100

    &_startAnimation
        &::after, &::before
            content attr(data-item)
            transition clip-path 5s linear, background-color .2s linear
            position absolute
            top 0
            bottom 0
            left 50%
            right 0
            transform translate3d(-50%, 0, 0)
            width 100%
            overflow hidden
            padding-left 1.5em
            padding-right 1.5em
            clip-path polygon(10% 0, 20% 0, 10% 100%, 0 100%)

        &::after
            animation titleBlick 6s linear 1s
            animation-fill-mode forwards

        &::before
            animation titleBlick2 1s linear 3.7s
            animation-fill-mode forwards

@keyframes titleBlick
    0%
        clip-path polygon(10% 0, 20% 0, 10% 100%, 0 100%)
        transform translate3d(-50%, 0, 0)
        opacity 1
    20%
        clip-path polygon(90% 0, 100% 0, 90% 100%, 80% 100%)
        transform translate3d(-40%, 0, 0)
        visibility hidden
    21%
        visibility hidden
    22%
        visibility hidden
        clip-path polygon(10% 0, 20% 0, 10% 100%, 0 100%)
    23%
        visibility hidden
    24%
        visibility visible
    30%
        clip-path polygon(10% 0, 20% 0, 10% 100%, 0 100%)
        transform translate3d(-40%, 0, 0)
    47%
        transform translate3d(-50%, 0, 0)
    50%
        clip-path polygon(10% 0, 100% 0, 90% 100%, 0 100%)
        opacity 1
    75%
        opacity 0
    100%
        clip-path polygon(10% 0, 100% 0, 90% 100%, 0 100%)
        opacity 0

@keyframes titleBlick2
    0%
        clip-path polygon(10% 0, 20% 0, 10% 100%, 0 100%)
    100%
        clip-path polygon(90% 0, 100% 0, 90% 100%, 80% 100%)

