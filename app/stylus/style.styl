.preloader
    position fixed
    top 0
    left 0
    width 100%
    height 100%
    display -webkit-flex
    display -moz-flex
    display -ms-flex
    display -o-flex
    display flex
    align-items center
    justify-content center
    z-index 2

    &_hide
        animation hidePreloader 1s

    &_hidden
        display none

    &__content
        position relative
        width 8em
        height 8em

    &__brick
        position absolute
        width 48%
        height 48%
        outline-offset -0.8em
        transform scale(.9)

        &_1
            top 0
            left 0
            animation brick1move 3s ease infinite
            animation-delay .25s

        &_2
            top 0
            left 52%
            animation brick2move 3s ease infinite

        &_3
            top 52%
            left 0
            animation brick3move 3s ease infinite
            animation-delay .5s

