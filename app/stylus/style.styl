    &_6
        .table__cell
            width 7.25em
            height 7.25em
            line-height 7.25em

        .cell__side
            &_right
                transform rotateY(90deg) translate3d(-1em, 0, 6.25em)

            &_bottom
                transform rotateX(-90deg) translate3d(0, -1em, 6.25em)

        .cell__number, .cell__sideNumber
            font-size 4em

        .cell__side_front
            outline-offset -1em

    &__row
        transform-style preserve-3d
        display -webkit-flex
        display -moz-flex
        display -ms-flex
        display -o-flex
        display flex
        justify-content space-between

    &__cell
        display inline-block

