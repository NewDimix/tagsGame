.startPage
    position fixed
    top 0
    left 0
    width 100%
    height 100%
    display -webkit-flex
    display -moz-flex
    display -ms-flex
    display -o-flex
    display flex
    align-items center
    justify-content center
    z-index 1

    &_hide
        animation hidePreloader .5s

        .startPage__btn
            transform scale(1)
            transition transform 1s linear, background-color 0s linear

    &_hidden
        display none

    &__btn
        font-size 150em
        border none
        font-family 'Exo2-300', sans-serif
        transform scale(.025)
        transition transform 1s ease, background-color .25s linear
        padding .15em 1em .35em

        &:hover
            cursor pointer

.header
    transition transform 1s ease 4.25s
    padding 1em
    max-width 1440px
    margin-left auto
    margin-right auto

    &_startPosition
        transform translate3d(0, -10em, 0)

    &_win
        .header__winText
            transform translate3d(0, 0, 0)
            opacity 1

    &__line
        display -webkit-flex
        display -moz-flex
        display -ms-flex
        display -o-flex
        display flex
        justify-content space-between
        align-items center

        &:not(:last-child)
            margin-bottom 1em

    &__progress, &__timer
        font-size 2em
        width 7em
        white-space nowrap

    &__progress
        &::before
            content 'Ходов: '

    &__winText
        font-size 2.5em
        transform translate3d(0, 2em, 0)
        opacity 0
        transition transform .5s ease, opacity 1s ease

    &__timer
        text-align right

.btn
    border none
    padding .3em .6em
    font-size 2em
    width 6em
    height 1.2em
    font-family 'Exo2-300', sans-serif
    position relative
    overflow hidden
    transition background-color .5s ease, transform .1s ease, border-bottom .1s ease, box-shadow .1s linear
    text-transform uppercase
    box-sizing content-box

    &::after
        content ''
        position absolute
        width 0%
        height 200%
        left -100%
        top -50%
        transform rotate(45deg)
        transform-origin center center

    &:enabled
        &:hover
            cursor pointer

            &::after
                animation blick 1s ease
                animation-fill-mode forwards

        &:active
            transform translate3d(0, .2em, 0)
            border-bottom-width 0

    &:focus
        outline none

    &_clickMe
        animation clickMe .5s ease
        animation-fill-mode forwards

@keyframes clickMe
    0%
        box-shadow 0 0 2em 0em rgba(169, 255, 248, 0), inset 0 0 0 1px rgba(64, 91, 118, 1)
        transform scale(1)
    50%
        box-shadow 0 0 2em 0em rgba(169, 255, 248, .5), inset 0 0 0 1px rgba(64, 91, 118, 1)
        transform scale(1.1)
    100%
        box-shadow 0 0 2em 0em rgba(169, 255, 248, .5), inset 0 0 0 1px rgba(64, 91, 118, 1)
        transform scale(1)

.field
    position relative
    perspective 500em
    transition transform .5s ease 4.25s
    font-size 5.25px
    -moz-user-select none
    -khtml-user-select none
    -webkit-user-select none
    -ms-user-select none
    user-select none

    &_startPosition
        transform translate3d(0, -5em, 0)

        .field__cube
            transform rotateX(-90deg) rotateY(360deg)

        .field__side_top
            background-color transparent

            &::after, &::before
                width 0

    &_defaultPosition
        .field__side_top
            transition none

            &::after, &::before
                display none

        .title
            display none

    &_glow
        .field__cube
            animation glow .8s ease
            animation-fill-mode backwards

    &__cube
        width 50em
        height 50em
        margin-left auto
        margin-right auto
        transform-style preserve-3d
        will-change transform
        transition transform 1.5s ease 4.25s, box-shadow .5s ease

        &.settingsRotate
            .field__side_bottom
                span
                    transform rotate(180deg)

    &__side
        position absolute
        transform-style preserve-3d
        backface-visibility hidden

        &_front
            width 50em
            height 50em
            transform translate3d(0, 0, 2.5em)

        &_back
            width 50em
            height 50em
            transform rotateY(180deg) translate3d(0, 0, 2.5em)

        &_right
            width 5em
            height 50em
            transform rotateY(90deg) translate3d(0, 0, 47.5em)

        &_left
            width 5em
            height 50em
            transform rotateY(-90deg) translate3d(0, 0, 2.5em)

        &_top
            width 50em
            height 5em
            line-height 5em
            transform rotateX(90deg) translate3d(0, 0, 2.5em)
            transition background-color .5s ease 4.1s, box-shadow 0s ease 4.1s
            text-align center

            &::after, &::before
                content ''
                position absolute
                height 1px
                width 100%
                left 50%
                transform translate3d(-50%, 0, 0)
                transition width 1s ease 3.25s

            &::after
                top 0

            &::before
                bottom 0

        &_bottom
            width 50em
            height 5em
            transform rotateX(-90deg) translate3d(0, 0, 47.5em)
            text-align center

            span
                font-size 2.5em
                line-height 2em
                filter opacity(.3)
                display inline-block

        &_timer
            left 50%
            top 50%
            transform rotateY(180deg) translate3d(-50%, -50%, 20em) scale(.2)
            backface-visibility visible
            box-shadow none

        &_blur
            left 50%
            top 50%
            box-shadow none

.blur
    width 50em
    height 50em
    transition opacity 0.5s, visibility 0s linear 0.5s
    background-color transparent
    opacity 0
    visibility hidden
    transform translateZ(5.5em) translate3d(-50%, -50%, 5.5em)
    backface-visibility hidden
    perspective 500em
    backdrop-filter blur(.2em)

    &_visible
        opacity 1
        visibility visible
        transition-delay .5s

.timerStart
    width 15em
    height 10em
    visibility hidden
    will-change transform
    position relative

    &_visible
        visibility visible

    &_front
        transition transform 3s cubic-bezier(0.050, 0.860, 0.110, 0.960)
        transform rotateY(0) translate3d(-50%, -50%, 30em) scale(1.5)

    &_back
        transition transform 0.5s ease-in
        transform rotateY(-180deg) translate3d(-50%, -50%, 60em) scale(.2)

    &__number
        position absolute
        left 50%
        top 50%
        transform translate3d(-50%, -50%, 0)
        width 2.6em
        height 9.1em

    &__part
        position absolute
        width 2.6em
        height .8em
        display none
        transition all .3s ease

        &::before, &::after
            content ''
            position: absolute;
            width: 0;
            height: 0;
            border: .4em solid transparent;
            transition all .3s ease

        &::before
            right -0.8em

        &::after
            left -0.8em

        &_1
            left 0
            top 0
            display block

        &_2
            left -1.8em
            top 2em
            display block
            transform rotate(-90deg)

        &_3
            right -1.8em
            top 2em
            display block
            transform rotate(90deg)

        &_4
            left -1.8em
            top 6.3em
            display block
            transform rotate(-90deg)

        &_5
            right -1.8em
            top 6.3em
            display block
            transform rotate(90deg)

        &_6
            left 0
            bottom 0
            display block
            transform rotate(180deg)

        &_7
            left 0
            top 50%
            display block
            transform rotate(180deg) translate3d(0, 50%, 0) scale(1.1)

            &::before
                border-top-color: transparent !important;
                right -0.8em

            &::after
                border-top-color: transparent !important;
                left -0.8em

@keyframes glow
    0%
        box-shadow 0 0 50em 0em rgba(169, 255, 248, 0)
    25%
        box-shadow 0 0 50em 0em rgba(169, 255, 248, 0.1)
    100%
        box-shadow 0 0 50em 0em rgba(169, 255, 248, 0)


.table
    padding 1.5em
    box-sizing border-box
    display -webkit-flex
    display -moz-flex
    display -ms-flex
    display -o-flex
    display flex
    justify-content space-between
    flex-direction column

    &_4
        .table__cell
            width 11em
            height 11em
            line-height 11em

        .cell__side
            &_right
                transform rotateY(90deg) translate3d(-1em, 0, 10em)

            &_bottom
